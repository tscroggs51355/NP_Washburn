#!/bin/bash
#SBATCH --job-name=Nelms2019_Salmon
#SBATCH --partition=batch
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=10
#SBATCH --time=72:00:00
#SBATCH --mem=70gb
#SBATCH --output=nelms2019.%j.out
#SBATCH --error=nelms2019.%j.err
#SBATCH --mail-type=END,FAIL
#SBATCH --mail-user=tms51355@uga.edu

ml Salmon 

salmon alevin -l ISR --whitelist CELseq_whitelist.txt --umi-geometry 1[1-10] --bc-geometry 1[11-18] --read-geometry 2[7-end] -i transcriptome_index -1 SRR7989734_2.fastq.gz -2 SRR7989734_1.fastq.gz -p 10 -o outputfile --tgMap TGMap.tsv

